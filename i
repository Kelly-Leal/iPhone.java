public class iPhone {
    public void play() {
        // tocar música
    }
    public void pause() {
        // pausar música
    }
    public void selectSong() {
        // selecionar uma música para tocar
    }
    public void call() {
        // fazer uma ligação telefônica
    }
    public void answer() {
        // atender uma ligação telefônica
    }
    public void startVoicemail() {
        // iniciar correio de voz
    }
    public void displayPage() {
        // exibir uma página da web
    }
    public void addNewTab() {
        // adicionar uma nova aba no navegador da web
    }
    public void refreshPage() {
        // atualizar a página da web atual
    }
}
